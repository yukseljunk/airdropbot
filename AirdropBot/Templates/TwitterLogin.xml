  <if compare="INPUT" what="tagName" xpath="//fieldset/div/input">
    <snap xpath="//fieldset/div/input" x="%50" y="%150"/>
    <wait for="${Random(1,3)}"/>
    <sendkey value="${0}"/>
    <wait for="${Random(1,3)}"/>
    <snap xpath="//fieldset/div[2]/input" x="%50" y="%150"/>
    <wait for="${Random(1,3)}"/>
    <sendkey value="${1}" />
    <wait for="${Random(1,3)}"/>
    <snap xpath="//button[@type='submit']" x="%50" y="%150"/>
    <wait for="${Random(4,8)}"/>

  </if>


</steps>